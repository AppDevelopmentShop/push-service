{
  "name": "push-service",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "babel-node ./src/index.js",
    "build": "babel src -d dist --source-maps",
    "knex": "babel-node node_modules/.bin/knex --knexfile=./src/knexfile.js",
    "db:migrate": "NODE_ENV=development npm run migrate:rollback && npm run migrate:latest && npm run seed:run",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed:run": "npm run knex seed:run",
    "start:docker": "NODE_ENV=development && npm run migrate:latest && npm run seed:run && npm run start",
    "start:prod": "node dist/index.js",
    "start:db": "npm run migrate:latest && npm run seed:run",
    "start:admin": "node dist/modules/push/index.js"
  },
  "keywords": [
    "push",
    "notifications",
    "mobile",
    "firebase"
  ],
  "author": "Alexander Stepanov",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "firebase-admin": "^5.12.0",
    "joi": "^13.3.0",
    "knex": "^0.14.5",
    "knexnest": "^0.4.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "pg": "^7.4.1",
    "winston": "^2.4.1"
  }
}
